{
  "name": "@lighthouse/abab",
  "version": "0.0.1",
  "description": "WHATWG spec-compliant implementations of window.atob and window.btoa.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "yarn clear && NODE_ENV=production parcel build src/index.js",
    "clear": "rimraf dist",
    "lint": "eslint src",
    "prepublish": "yarn build",
    "preversion": "yarn test",
    "test": "jest test",
    "test:ci": "yarn lint && yarn test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsdom/abab.git"
  },
  "keywords": [
    "atob",
    "btoa"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/jsdom/abab/issues"
  },
  "homepage": "https://github.com/jsdom/abab#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "jest": "^23.5.0",
    "jest-junit-reporter": "^1.1.0",
    "mocha": "^5.1.0",
    "parcel-bundler": "^1.9.7",
    "rimraf": "^2.6.2"
  }
}
